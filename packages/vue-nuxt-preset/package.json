{
  "name": "@xstorybook/vue-nuxt-preset",
  "version": "0.1.1",
  "description": "Storybook preset for Nuxt apps",
  "keywords": [
    "storybook-preset",
    "nuxt",
    "vue"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "@storybook/builder-vite": "9.0.10",
    "@storybook/vue3": "9.0.10",
    "@storybook/vue3-vite": "9.0.10",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "pathe": "^1.1.2",
    "pkg-types": "^1.1.1",
    "read-pkg-up": "^10.1.0",
    "semver": "^7.6.3",
    "stringify-object": "^5.0.0",
    "vite": "~6.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.16.2",
    "@types/semver": "^7.5.8",
    "@types/stringify-object": "^4.0.5",
    "storybook": "9.0.10",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vue": "^3.5.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0",
    "storybook": "9.0.10",
    "vue": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "external": [
      "nuxt",
      "vue",
      "vite",
      "storybook"
    ]
  }
}